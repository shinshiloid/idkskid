        for i, molly in pairs(rayignore:FindFirstChild("Fires"):GetChildren()) do 
            molly.Transparency = 0
            molly.Color = Options.misc_mollycolor.Value
        end 
    else 
        for i, molly in pairs(rayignore:FindFirstChild("Fires"):GetChildren()) do 
            molly.Transparency = 1 
        end 
    end
end)
